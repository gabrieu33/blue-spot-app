<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="MyBottomNavigation" parent="Widget.MaterialComponents.BottomNavigationView">
        <item name="colorPrimary">#195073</item> <!-- Define a cor de fundo quando selecionado -->
        <item name="itemTextColor">@color/white</item> <!-- Cor do texto não selecionado -->
        <item name="itemIconTint">@color/white</item> <!-- Cor do ícone não selecionado -->
    </style>

    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="android:windowTranslucentStatus">true</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
    </style>

    <style name="circular_borda">
        <item name="cornerSize">50%</item>


    </style>
    <style name="textlocal">
        <item name="android:textColorHint">@color/black</item>
    </style>

    <style name="Statubar" parent="Theme.AppCompat.Light.DarkActionBar">
       <!-- ... outras configurações de estilo ... -->
        <item name="android:statusBarColor">@color/azul_principal</item>

    </style>

</resources>